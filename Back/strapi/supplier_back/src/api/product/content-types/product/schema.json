{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Variant": {
      "displayName": "Variant product",
      "type": "component",
      "repeatable": true,
      "component": "variant-product.variant-product"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "supplier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    },
    "price": {
      "required": true,
      "type": "decimal"
    },
    "Variant_type_name": {
      "type": "string"
    },
    "stock_total": {
      "type": "integer"
    },
    "main_images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
